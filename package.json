{
  "name": "@jacekpietal/oneforall",
  "description": "set of classes to better organize 2d game development",
  "version": "3.1.5",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "dependencies": {
    "@jacekpietal/dependency-injection": "^1.4.0",
    "detect-collisions": "^6.6.8",
    "latermom": "^1.0.5",
    "rxjs": "^7.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.5",
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.4",
    "@types/offscreencanvas": "^2019.7.0",
    "babel-jest": "^29.3.1",
    "chef-express": "^1.3.0",
    "jest": "29.3.1",
    "pixi-shim": "^2.4.0",
    "pixi.js": "^6",
    "pixi.js-legacy": "^6",
    "prettier": "^2.8.0",
    "ts-loader": "^9.4.2",
    "tslint": "^6.1.3",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "scripts": {
    "start": "chef-express docs",
    "lint": "yarn tslint 'src/**/*.ts' --fix",
    "test": "jest --silent --verbose --forceExit",
    "sanity": "yarn build && node ./test-build.js",
    "demo": "webpack && yarn start",
    "format": "prettier ./src/*.ts --write",
    "format-docs": "prettier ./docs/*.ts --write",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "precommit": "yarn lint;yarn sanity && yarn format && yarn format-docs && yarn test && webpack"
  },
  "keywords": [
    "gameobject",
    "prefab",
    "state",
    "machine",
    "sprite",
    "circle",
    "body",
    "polygon",
    "physics",
    "container",
    "scene",
    "animator",
    "webgl",
    "pixi.js"
  ],
  "repository": "git://github.com/Prozi/oneforall.git"
}
