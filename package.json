{
  "name": "@jacekpietal/oneforall",
  "description": "set of classes to better organize 2d game development",
  "version": "1.11.5",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "dependencies": {
    "@jacekpietal/dependency-injection": "^1.4.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/offscreencanvas": "^2019.6.4",
    "chef-express": "^1.1.0",
    "detect-collisions": "^3.2.7",
    "esm": "^3.2.25",
    "jest": "^27.5.1",
    "latermom": "^1.0.4",
    "pixi-shim": "^2.3.21",
    "pixi.js": "~6.0.4",
    "pixi.js-legacy": "~6.0.4",
    "prettier": "^2.6.2",
    "prettier-config-standard": "^5.0.0",
    "rxjs": "^7.5.5",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "scripts": {
    "start": "chef-express docs",
    "test": "jest --verbose --forceExit",
    "test-build": "yarn build && node -r esm ./test-build.js",
    "test-docs": "webpack && cp dist/demo/demo.bundle.js docs && yarn start",
    "format": "prettier ./src/*.ts --write",
    "format-docs": "prettier ./docs/*.ts --write",
    "build": "tsc",
    "precommit": "yarn format && yarn format-docs && yarn test-build && yarn test && yarn test-docs"
  },
  "keywords": [
    "gameobject",
    "prefab",
    "state",
    "machine",
    "sprite",
    "circle",
    "body",
    "polygon",
    "physics",
    "container",
    "scene",
    "animator",
    "webgl",
    "pixi.js"
  ],
  "repository": "git://github.com/Prozi/oneforall.git"
}
