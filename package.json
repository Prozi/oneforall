{
  "name": "@jacekpietal/oneforall",
  "description": "set of classes to better organize 2d game development",
  "version": "3.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "dependencies": {
    "@jacekpietal/dependency-injection": "^1.4.0",
    "detect-collisions": "^6.0.2",
    "latermom": "^1.0.5",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.18.6",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^28.1.4",
    "@types/offscreencanvas": "^2019.7.0",
    "babel-jest": "^28.1.2",
    "chef-express": "^1.2.0",
    "jest": "^28.1.2",
    "pixi-shim": "^2.3.24",
    "pixi.js": "6.4.2",
    "pixi.js-legacy": "6.4.2",
    "prettier": "^2.7.1",
    "ts-loader": "^9.3.1",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "scripts": {
    "start": "chef-express docs",
    "lint": "yarn tslint 'src/**/*.ts' --fix",
    "test": "jest --silent --verbose --forceExit",
    "sanity": "yarn build && node ./test-build.js",
    "demo": "webpack && yarn start",
    "format": "prettier ./src/*.ts --write",
    "format-docs": "prettier ./docs/*.ts --write",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "precommit": "yarn lint;yarn sanity && yarn format && yarn format-docs && yarn test && webpack"
  },
  "keywords": [
    "gameobject",
    "prefab",
    "state",
    "machine",
    "sprite",
    "circle",
    "body",
    "polygon",
    "physics",
    "container",
    "scene",
    "animator",
    "webgl",
    "pixi.js"
  ],
  "repository": "git://github.com/Prozi/oneforall.git"
}
